<!-- Table of Contents -->
<% if (page.catalog || true) { %>
    <% if (is_post()){ %>
      <aside id="sidebar">
        <div id="toc" class="toc-article">
        <strong class="toc-title"><%= __('contents') %></strong>
        <%- toc(page.content, { "class": "toc-nav" , "list_number" : ""}) %>
        </div>
      </aside>
    <% } %>
<% } %>
